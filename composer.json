          {
              "type": "vcs",
              "url": "https://github.com/JulianSansat/pagseguro-php-sdky"
          }
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "PagSeguro"
    }
  ],
  "require": {
    "uol-pagseguro-moscou/pagseguro-php-sdk": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "PagSeguro\\": "source/"
    }
  },
  "autoload-dev": {
