{
  "name": "JulianSansat/pagseguro-php-sdk",
  "description": "Biblioteca de integração com o PagSeguro",
  "repositories":
  [
          {
              "type": "vcs",
              "url": "https://github.com/JulianSansat/pagseguro-php-sdky"
          }
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "PagSeguro"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-curl": "*",
    "lib-curl": "*",
    "lib-openssl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "PagSeguro\\": "source/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PagSeguro\\Tests\\": "tests/"
    }
  }
}
